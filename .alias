alias ls='ls --color=auto  --group-directories-first'
alias grep='grep --color=auto'
alias l='ls -l'
alias tidyxml='tidy -utf8 -xml -i --indent-spaces 4 -w 255 -q -m'
alias exdiff='svn diff | grep "^\-\|\+" | grep -v \@\@'
alias work='cd /usr/local/release'
alias tools='cd /svn/it/scripts/exports/trunk'
alias devtools='cd  "$HOME/workspace/Dev Release Tools/exports/branches/release-dev"'
alias dgb-trunk='cd /svn/it/scripts/exports/trunk && svn update'
alias dgb-qast='cd /svn/it/scripts/exports/qast && svn update'
alias dgb-prod='cd /svn/it/scripts/exports/prod && svn update'
