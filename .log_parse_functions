#!/bin/bash

# $1 - site name/seach term
# $2 - filename
function blog() {
    grep $1 $2 | tail -20 | sed -re 's/(.*)( \"Mozilla.*)/\1/' | grep httperror;
}
function clog() {
    grep $1 $2 | sed -e 's|\(.*http\: \)\(.*\)\(\[.*\] \)\(.*\)\( \"Mozilla.*\)|\1\2\4|';
}

# $1 filename
function tlog() {
    tail -f $1 | grep httperror | sed -e 's/\(.*httperror: \)\(.*\)/\2/';
}
